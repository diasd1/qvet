      "description": "Kreisfrequenz ω berechnen"
    },
    "PRI": {
      "formula": "P = R * I ^ 2",
      "description": "Leistung P durch Widerstand R und Strom I bestimmen"
    },
    "PUI": {
      "formula": "P = U * I",
      "description": "Leistung P durch Spannung U und Strom I bestimmen"
    },
    "RRlA": {
      "formula": "R = (ρ * l) / A",
      "description": "Elektrischer Widerstand vom Leiter bestimmen"
    },
    "RShuntRiISollIEnde1": {
      "formula": "R_Shunt = R_i / ((I_Soll / I_Ende) - 1)",
