    },
    "RWR201aDT": {
      "formula": "R_W = R_20 * (1 + α * ΔT)",
      "description": "Widerstand bei Wärme berechnen"
    },
    "T1f": {
      "formula": "T = 1 / f",
      "description": "Periodendauer berechnen"
    },
    "URI": {
      "formula": "U = R * I",
      "description": "Spannung U durch Widerstand R und Strom I bestimmen"
    },
    "UssUeff22": {
      "formula": "U_ss = (U_eff * sqrt(2)) * 2",
      "description": "Spitzenspitzenspannung berechnen"
    },
    "UsUeff2": {
      "formula": "U_s = U_eff * sqrt(2)",
      "description": "Spitzenspannung berechnen"
    },
    "WPt": {
      "formula": "W = P * t",
      "description": "Arbeit W durch Leistung P und Zeit t bestimmen"
